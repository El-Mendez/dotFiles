[core]
	longpaths = true
	editor = nvim
	autocrlf = input
[commit]
	gpgSign = true
[tag]
	forceSignAnnotated = true
[alias]
	graph = log --graph --decorate --pretty=oneline --abbrev-commit --all --date=local
	sw = switch -
[gpg]
	format = ssh
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[init]
	defaultBranch = main
